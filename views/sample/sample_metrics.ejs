            },
            yaxis: {
                min: 0, max: 70, tickSize: 20
            },
            grid: {
                borderWidth: 0
            }
        };

        // Charts option
        var chartUsersOptions2 = {
            lines: {
                show: true,
                fill: false,
                lineWidth: 2
            },
            yaxis: {
                min: 0, max: 100, tickSize: 20
            },
            grid: {

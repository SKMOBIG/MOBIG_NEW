        $.plot($("#metric7"), [data1], chartUsersOptions);
        $.plot($("#metric8"), [data3], chartUsersOptions);
        $.plot($("#metric9"), [data4], chartUsersOptions);

        function update() {
            plot5.setData([getRandomData()]);
            plot5.draw();
            setTimeout(update, updateInterval);
        }

        // Run update method

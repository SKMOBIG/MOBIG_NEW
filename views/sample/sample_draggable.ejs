<script>
    $(document).ready(function () {


        var element = ".draggable-container [class*=col]";
        var handle = ".panel-body";
        var connect = "[class*=col]";
        $(element).sortable(
                {
                    handle: handle,
                    connectWith: connect,
                    tolerance: 'pointer',
                    forcePlaceholderSize: true,
                    opacity: 0.8
                })
                .disableSelection();

    });
</script>

